<html>
	<body>
		<div id='container'>
		<div id='menus'>
			<%= render 'login' -%>
			<%= render 'list_posts' -%>
		</div>
			<div id='conteudo'>
				<% @posts.each do |p| %>
					<div id='post'>
						<div> <%= p.titulo.html_safe -%> </div>
						<div> <%= p.conteudo.html_safe -%> </div>
					</div>
				<% end %>
			</div>
		</div>
	</body>
</html>